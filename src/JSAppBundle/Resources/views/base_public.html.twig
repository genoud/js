<!-- Sparkline -->
<script src="{{ asset('resources/vendor/AdminLTE-2.3.6/plugins/sparkline/jquery.sparkline.min.js') }}"></script>
<!-- jvectormap -->
<script src="{{ asset('resources/vendor/AdminLTE-2.3.6/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js') }}"></script>
<script src="{{ asset('resources/vendor/AdminLTE-2.3.6/plugins/jvectormap/jquery-jvectormap-world-mill-en.js') }}"></script>
<!-- SlimScroll 1.3.0 -->
<script src="{{ asset('resources/vendor/AdminLTE-2.3.6/plugins/slimScroll/jquery.slimscroll.min.js') }}"></script>
<!-- ChartJS 1.0.1 -->
<script src="{{ asset('resources/vendor/AdminLTE-2.3.6/plugins/chartjs/Chart.min.js') }}"></script>
<!-- AdminLTE dashboard demo (This is only for demo purposes) -->
<script src="{{ asset('resources/vendor/AdminLTE-2.3.6/dist/js/pages/dashboard2.js') }}"></script>
<!-- AdminLTE for demo purposes -->
<script src="{{ asset('resources/vendor/AdminLTE-2.3.6/dist/js/demo.js') }}"></script>

