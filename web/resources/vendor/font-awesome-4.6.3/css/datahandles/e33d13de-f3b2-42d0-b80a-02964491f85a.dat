    <scriptTask
        code="CMN_PEPSCCDetails $pepSCCDetails := $party.kycProfile.pepSCCDetails;&#xa;String $error := '';&#xa;If $pepSCCDetails == null Then&#xa;   $error := 'Associated PEP Party is missing';&#xa;ElseIf EMPTY($pepSCCDetails.associatedPEPID) Then&#xa;   $error := 'Associated PEP Party ID is missing';&#xa;End&#xa;If EMPTY($error) &amp;&amp; not OR(NotEmpty($pepSCCDetails.associatedPEPLastName), NotEmpty($pepSCCDetails.associatedPEPName), NotEmpty($pepSCCDetails.associatedPEPNationality)) Then&#xa;   INT_SILCheckPersonResponse $response := INT_SILProxyClient_version.checkPerson($pepSCCDetails.associatedPEPID);&#xa;   If $response.getLastCallHasSucceededAndHasContent() Then&#xa;      SIL_INVOLVEDPARTYinvolvedParty $silAssociatedPEP := $response.silInvolvedPartySummary;&#xa;      If ISA($silAssociatedPEP, SIL_INVOLVEDPARTYindividual) Then&#xa;         If LIB_Validator.validateNotEmptyIndexed($silAssociatedPEP.names) &amp;&amp; ISA($silAssociatedPEP.names[1], SIL_INVOLVEDPARTYindividualName) Then&#xa;            $pepSCCDetails.associatedPEPLastName := CAST(SIL_INVOLVEDPARTYindividualName, $silAssociatedPEP.names[1]).lastName;&#xa;            $pepSCCDetails.associatedPEPName := CAST(SIL_INVOLVEDPARTYindividualName, $silAssociatedPEP.names[1]).firstName;&#xa;         End&#xa;         If LIB_Validator.validateNotEmptyIndexed($silAssociatedPEP.nationalities) &amp;&amp; NotEmpty($silAssociatedPEP.nationalities[1].code) Then&#xa;            //$pepSCCDetails.associatedPEPNationality := LIST($silAssociatedPEP.nationalities[1].code, 'CMN_Countries__selector', 'description');&#xa;            $pepSCCDetails.associatedPEPNationality := $silAssociatedPEP.nationalities[1].code;&#xa;            LIB_Logger.tmpLog('Nationality code : ' &amp; $pepSCCDetails.associatedPEPNationality);&#xa;         Else&#xa;            $error := 'Party ' &amp; $pepSCCDetails.associatedPEPID &amp; ' does not have a nationality.';&#xa;            $root.addErrorNotification($error, false);&#xa;         End&#xa;         If EMPTY($pepSCCDetails.associatedPEPLastName) and EMPTY($pepSCCDetails.associatedPEPName) Then&#xa;            $error := 'Party ' &amp; $pepSCCDetails.associatedPEPID &amp; ' does not have a First Name and a Last Name';&#xa;            $root.addErrorNotification($error, false);&#xa;         End&#xa;      Else&#xa;         $error := 'ID ' &amp; $pepSCCDetails.associatedPEPID &amp; ' is not a Physical Person';&#xa;         $root.addErrorNotification($error, false);&#xa;      End&#xa;   Else&#xa;      String $errorCode := ERROR($response.getLastServiceStatus().statusCode.getStatusErrorMessageFromResponse(), '');&#xa;      String $extendedError := ERROR($response.getLastServiceStatus().httpResponse.exception.getMessage().split('\\[')[1], '');&#xa;      If not $response.hasFailed() Then&#xa;         $error := 'ID ' &amp; $pepSCCDetails.associatedPEPID &amp; ' was not found in Banking System ' &amp; $errorCode;&#xa;         $root.addErrorNotification($error, false, $extendedError);&#xa;      ElseIf $response.getLastServiceStatus().httpResponse.exception != null Then&#xa;         $error := 'An Exception Occured ' &amp; $errorCode;&#xa;         $root.addErrorNotification($error, false, $extendedError);&#xa;      ElseIf not $response.getLastCallHasSucceededAndHasContent() Then&#xa;         $root.addErrorNotification('Banking System not properly responding ' &amp; $errorCode, false, $extendedError);&#xa;      End&#xa;   End&#xa;Else&#xa;   $root.addErrorNotification($error);&#xa;End"
