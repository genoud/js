        <method name="prebookPerson" userId="bernard" uuid="00000000-0798-3281-0000-000000000000">
            <definition><![CDATA[/**
  Prebook a person number. This is case dependant. 
  @param $party is the party for which a number has to be prebooked
  @return       Response object containing the prebooking ID.
*/
StaticFunction prebookPerson(String $type) : INT_SILPreBookPersonResponse Begin
   INT_SILPreBookPersonResponse $silResponse := NewObject(INT_SILPreBookPersonResponse);
   INT_HttpResponseWrapper $httpResponse;
   String $requestBody;
   Long $before := TIMESTAMP();
   /*********************** DEFINE PERSON TYPE **************************************/
   If EMPTY($type) Then
      Return $silResponse;
   ElseIf EQUAL($type, CMN_PartyType.typePhysicalPerson()) Then
      SIL_INVOLVEDPARTYindividual $silIndividualType := CAST(SIL_INVOLVEDPARTYindividual, CreateSkeletonDataClass(SIL_INVOLVEDPARTYindividual));
      $requestBody := TranslateTo($silIndividualType, 'json');
   ElseIf EQUAL($type, CMN_PartyType.typeTrust()) Then
      SIL_INVOLVEDPARTYtrust $silOrganizationType := CAST(SIL_INVOLVEDPARTYtrust, CreateSkeletonDataClass(SIL_INVOLVEDPARTYtrust));
      $requestBody := TranslateTo($silOrganizationType, 'json');
   Else
      SIL_INVOLVEDPARTYorganization $silOrganizationType := CAST(SIL_INVOLVEDPARTYorganization, CreateSkeletonDataClass(SIL_INVOLVEDPARTYorganization));
      $requestBody := TranslateTo($silOrganizationType, 'json');
   End
   /*********************** POST REQUEST FOR NEW ID *********************************/
   LIB_Logger.debug('In prebookPerson making a postInvolvedPartyPrebook ');
   $httpResponse := INT_SILHttpClient.postInvolvedPartyPrebook($requestBody);
   $silResponse.addServiceStatusFromHttpResponseWrapper($httpResponse);
   /*********************** DESERIALIZE ANSWER **************************************/
   If $silResponse.hasSucceded() Then
      Try
         If $silResponse.getLastServiceStatus().hasContent() Then
            $silResponse.involvedPartySummary := TranslateFrom($httpResponse.responseBody, 'SIL_INVOLVEDPARTYinvolvedParty', 'json');
            $silResponse.personId := $silResponse.involvedPartySummary.id;
         Else
            FAIL('Missing HTTP Response body');
            Long $end := TIMESTAMP();
            LIB_Logger.warn('FAIL Time taken to complete Prebook request in milliseconds: ' & TOSTRING(($end - $before)));
         End
      Catch
         LIB_Logger.integrationError('[INT_SILProxyClient]: Error while deserializing:  ' & $exception.getMessage());
         $silResponse.addServiceStatusFromException($exception);
      End
   End
   Long $end := TIMESTAMP();
   LIB_Logger.warn('Time taken to complete prebookPerson request in milliseconds: ' & TOSTRING(($end - $before)));
   Return $silResponse;
End]]></definition>
        </method>
