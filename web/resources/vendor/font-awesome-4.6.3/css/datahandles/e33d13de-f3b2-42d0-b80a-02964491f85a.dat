    <scriptTask
        code="Integer $counter := 1;&#xa;Indexed String $idCRMs := NewIndexed(String);&#xa;Named INT_SILCheckPersonResponse $checkConnectedPartyResponse := NewNamed(INT_SILCheckPersonResponse);&#xa;ForEach CMN_ConnectedParty $connectedParty In $party.kycProfile.connectedParties.connectedParty Do&#xa;   String $error := '';&#xa;   If $connectedParty == null Then&#xa;      $error := 'Connected Party ' &amp; $counter &amp; ' is missing';&#xa;   ElseIf EMPTY($connectedParty.idCRM) Then&#xa;      $error := 'Connected Party ' &amp; $counter &amp; ' ID CRM is missing';&#xa;   End&#xa;   If EMPTY($error) and EMPTY($connectedParty.fullName) and not $idCRMs.contains($connectedParty.idCRM) Then&#xa;      $idCRMs.addElement($connectedParty.idCRM);&#xa;   ElseIf NotEmpty($error) Then&#xa;      $root.addErrorNotification($error);&#xa;   End&#xa;   $counter := $counter + 1;&#xa;End&#xa;INT_SILCheckPersonResponse $response;&#xa;ForEach String $idCRM In $idCRMs Do&#xa;   String $error := '';&#xa;   $response := INT_SILProxyClient_version.checkPerson($idCRM);&#xa;   If $response.getLastCallHasSucceededAndHasContent() Then&#xa;      $checkConnectedPartyResponse[$idCRM] := $response;&#xa;   Else&#xa;      String $errorCode := ERROR($response.getLastServiceStatus().statusCode.getStatusErrorMessageFromResponse(), '');&#xa;      String $extendedError := ERROR($response.getLastServiceStatus().httpResponse.exception.getMessage().split('\\[')[1], '');&#xa;      If not $response.hasFailed() Then&#xa;         $error := 'Connected Party with ID ' &amp; $idCRM &amp; ' was not found in Banking System ' &amp; $errorCode;&#xa;         $root.addErrorNotification($error, false, $extendedError);&#xa;      ElseIf $response.getLastServiceStatus().httpResponse.exception != null Then&#xa;         $error := 'An Exception Occured ' &amp; $errorCode &amp; ' when loading Connected Party with CRM ID' &amp; $idCRM;&#xa;         $root.addErrorNotification($error, false, $extendedError);&#xa;      ElseIf not $response.getLastCallHasSucceededAndHasContent() Then&#xa;         $root.addErrorNotification('Banking System not properly responding ' &amp; $errorCode &amp; ' when loading Connected Party with CRM ID' &amp; $idCRM, false, $extendedError);&#xa;      End&#xa;   End&#xa;End&#xa;$counter := 1;&#xa;ForEach CMN_ConnectedParty $connectedParty In $party.kycProfile.connectedParties.connectedParty Do&#xa;   String $error := '';&#xa;   $response := $checkConnectedPartyResponse[$connectedParty.idCRM];&#xa;   If $response != null Then&#xa;      SIL_INVOLVEDPARTYinvolvedParty $silConnectedParty := $response.silInvolvedPartySummary;&#xa;      If ISA($silConnectedParty, SIL_INVOLVEDPARTYindividual) Then&#xa;         $connectedParty.setType(CMN_ConnectedParty.typeIndividual());&#xa;         If LIB_Validator.validateNotEmptyIndexed($silConnectedParty.names) and OR(NotEmpty(CAST(SIL_INVOLVEDPARTYindividualName, $silConnectedParty.names[1]).firstName), NotEmpty(CAST(SIL_INVOLVEDPARTYindividualName, $silConnectedParty.names[1]).lastName)) Then&#xa;            If NotEmpty($silConnectedParty.names[1].fullName) Then&#xa;               $connectedParty.fullName := ERROR($silConnectedParty.names[1].fullName, '');&#xa;            Else&#xa;               $connectedParty.fullName := CAST(SIL_INVOLVEDPARTYindividualName, $silConnectedParty.names[1]).firstName &amp; ' ' &amp; CAST(SIL_INVOLVEDPARTYindividualName, $silConnectedParty.names[1]).lastName;&#xa;            End&#xa;         Else&#xa;            $error := 'Connected Party ' &amp; $counter &amp; ' with Id ' &amp; $connectedParty.idCRM &amp; ' does not have a name.';&#xa;            $root.addErrorNotification($error, false);&#xa;         End&#xa;      ElseIf ISA($silConnectedParty, SIL_INVOLVEDPARTYtrust) Then&#xa;         $connectedParty.setType(CMN_ConnectedParty.typeOrganization());&#xa;         If LIB_Validator.validateNotEmptyIndexed($silConnectedParty.names) and OR(NotEmpty(CAST(SIL_INVOLVEDPARTYtrustName, $silConnectedParty.names[1]).fullName), NotEmpty(CAST(SIL_INVOLVEDPARTYtrustName, $silConnectedParty.names[1]).name)) Then&#xa;            If NotEmpty($silConnectedParty.names[1].fullName) Then&#xa;               $connectedParty.fullName := ERROR($silConnectedParty.names[1].fullName, '');&#xa;            Else&#xa;               $connectedParty.fullName := CAST(SIL_INVOLVEDPARTYtrustName, $silConnectedParty.names[1]).name;&#xa;            End&#xa;         Else&#xa;            $error := 'Connected Party ' &amp; $counter &amp; ' with Id ' &amp; $connectedParty.idCRM &amp; ' does not have a name.';&#xa;            $root.addErrorNotification($error, false);&#xa;         End&#xa;      ElseIf ISA($silConnectedParty, SIL_INVOLVEDPARTYorganization) Then&#xa;         $connectedParty.setType(CMN_ConnectedParty.typeOrganization());&#xa;         If LIB_Validator.validateNotEmptyIndexed($silConnectedParty.names) and OR(NotEmpty($silConnectedParty.names[1].fullName), NotEmpty(CAST(SIL_INVOLVEDPARTYorganizationName, $silConnectedParty.names[1]).businessName)) Then&#xa;            If NotEmpty($silConnectedParty.names[1].fullName) Then&#xa;               $connectedParty.fullName := ERROR($silConnectedParty.names[1].fullName, '');&#xa;            Else&#xa;               $connectedParty.fullName := CAST(SIL_INVOLVEDPARTYorganizationName, $silConnectedParty.names[1]).businessName;&#xa;            End&#xa;         Else&#xa;            $error := 'Connected Party ' &amp; $counter &amp; ' with Id ' &amp; $connectedParty.idCRM &amp; ' does not have a name.';&#xa;            $root.addErrorNotification($error, false);&#xa;         End&#xa;      Else&#xa;         $error := 'Connected Party ' &amp; $counter &amp; ' with Id ' &amp; $connectedParty.idCRM &amp; ' is unknown type.';&#xa;         $root.addErrorNotification($error, false);&#xa;      End&#xa;   Else&#xa;   End&#xa;   $counter := $counter + 1;&#xa;End"
