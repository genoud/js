            <definition><![CDATA[StaticFunction testStoreRelation(COB_Root $root, String $accountId := '') : TTLS_TestResultWrapper Begin
   TTLS_TestResultWrapper $result := NewObject(TTLS_TestResultWrapper);
   CMN_BusinessRelationship $cmnBusinessRelationship := $root.getBusinessRelationship();
   If NotEmpty($accountId) Then
      $cmnBusinessRelationship.setSILID($accountId);
   ElseIf EMPTY($cmnBusinessRelationship.getSILID()) Then
      $cmnBusinessRelationship.setSILID('B1D312G');
   End
   ForEach CMN_Party $cmnParty In $cmnBusinessRelationship.getInvolvedParties() Do
      IF(EMPTY($cmnParty.getSILID()), $cmnParty.setSILID('PQUU5FE'));
   End
   INT_SILStoreRelationResponse $response := INT_SILProxyClient_version.createRelation($cmnBusinessRelationship);
   $result.proxyResult := $response;
   Return $result;
End]]></definition>
