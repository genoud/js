    <scriptTask
        code="Integer $counter := 1;&#xa;ForEach CMN_LinkedAccount $linkedAccount In $party.linkedAccounts Do&#xa;   String $error := '';&#xa;   If $linkedAccount == null Then&#xa;      $error := 'Linked Account ' &amp; $counter &amp; ' is missing';&#xa;   ElseIf EMPTY($linkedAccount.accountNumber) Then&#xa;      $error := 'Linked Account ' &amp; $counter &amp; 'Number is missing';&#xa;   End&#xa;   If EMPTY($error) and ($linkedAccount.accountActivity == null or not $linkedAccount.activityRetrieved) Then&#xa;      INT_SILReadAccountActivityResponse $proxyResponse := INT_SILProxyClient_version.readAccountActivity($linkedAccount.accountNumber);&#xa;      If $proxyResponse.getLastCallHasSucceededAndHasContent() Then&#xa;         $linkedAccount.activityRetrieved := true;&#xa;         $linkedAccount.accountActivity := $proxyResponse.kytActualAccountActivity;&#xa;      Else&#xa;         String $errorCode := ERROR($proxyResponse.getLastServiceStatus().statusCode.getStatusErrorMessageFromResponse(), '');&#xa;         String $extendedError := ERROR($proxyResponse.getLastServiceStatus().httpResponse.exception.getMessage().split('\\[')[1], '');&#xa;         If not $proxyResponse.hasFailed() Then&#xa;            $error := 'Linked Account Number ' &amp; $linkedAccount.accountNumber &amp; ' was not found in Banking System ' &amp; $errorCode;&#xa;            $root.addErrorNotification($error, false, $extendedError);&#xa;         ElseIf $proxyResponse.getLastServiceStatus().httpResponse.exception != null Then&#xa;            $error := 'An Exception Occured ' &amp; $errorCode;&#xa;            $root.addErrorNotification($error, false, $extendedError);&#xa;         ElseIf not $proxyResponse.getLastCallHasSucceededAndHasContent() Then&#xa;            $error := 'Banking System not properly responding ' &amp; $errorCode;&#xa;            $root.addErrorNotification($error, false, $extendedError);&#xa;         Else&#xa;            $error := 'Banking System Error unknown ' &amp; $errorCode;&#xa;            $root.addErrorNotification($error, false, $extendedError);&#xa;         End&#xa;      End&#xa;   ElseIf NotEmpty($error) Then&#xa;      $root.addErrorNotification($error);&#xa;   End&#xa;   $counter := $counter + 1;&#xa;End"
