        <method name="readPerson" userId="pezzi" uuid="ffffffff-dc3d-d88b-0000-000000000000">
            <definition><![CDATA[/**
  Read a Person Instance from SIL Backend System
  @id 			SIL Id of the person
  @return       Representation of the person instance. 
*/
StaticFunction readPerson(String $partyId, String $kycLevel, INT_SILReadPartyResponse $response := null, CMN_Party $cmnParty := null, CMN_ClientLifecycleData $cld := null) : INT_SILReadPartyResponse Begin
   Long $before := TIMESTAMP();
   If $response == null Then
      $response := INT_SILProxyClientBKP.readPersonIDV($partyId, $cmnParty);
   End
   If not $response.getLastCallHasSucceededAndHasContent() Then
      Return $response;
   End
   INT_HttpResponseWrapper $httpResponse;
   /***************** GENERIC FIELDS ************************************************/
   SIL_INVOLVEDPARTYinvolvedParty $silInvolvedParty;
   SIL_INVOLVEDPARTYCustomerRole $mainRole := CAST(SIL_INVOLVEDPARTYCustomerRole, CreateSkeletonDataClass(SIL_INVOLVEDPARTYCustomerRole));
   SIL_INVOLVEDPARTYpartyStructure $partyStructure := CAST(SIL_INVOLVEDPARTYpartyStructure, CreateSkeletonDataClass(SIL_INVOLVEDPARTYpartyStructure));
   Indexed SIL_INVOLVEDPARTYbusinessOwner $ownedBusinesses := NewIndexed(SIL_INVOLVEDPARTYbusinessOwner);
   CMN_SilIds $silIds;
   If $cld == null Then
      $silIds := NEW(CMN_SilIds);
   Else
      $silIds := $cld.getSILIDs($partyId);
      If $silIds == null Then
         $cld._silIds[$partyId] := NEW(CMN_SilIds);
         $silIds := $cld.getSILIDs($partyId);
      End
   End
   Try
      If $response.isIndividual() Then
         /**************************** BASIC KYC *************************/
         If EQUAL(CMN_KYCLevel.basic(), $kycLevel) Then
         End
         /**************************** FULL KYC *************************/
         If EQUAL(CMN_KYCLevel.full(), $kycLevel) Then
            /***************** GET SOW *********************************************/
            $httpResponse := INT_SILHttpClient.getInvolvedPartyIdSOW($partyId);
            $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
            If $response.getLastCallHasSucceededAndHasContent() Then
               $response.silSOW := TranslateFrom($httpResponse.responseBody, 'SIL_SOWindividualSourceOfWealth:indexed', 'json');
            End
         End
      ElseIf $response.isOrganization() Then
         If EQUAL(CMN_KYCLevel.basic(), $kycLevel) Then
         End
         If EQUAL(CMN_KYCLevel.full(), $kycLevel) Then
            If $response.isCommercialOperatingBusiness() Then
               /***************** CORPORATE STRUCTURE **************************************/
               /***************** BEARER SHARES **************************************/
               /***************** NATURE OF BUSINESS AND SORUCE OF WEALTH **************************************/
               $httpResponse := INT_SILHttpClient.getInvolvedPartyIdBusinessOrganization($partyId);
               $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
               If $response.getLastCallHasSucceededAndHasContent() Then
                  $response.silBusinessOrganization := TranslateFrom($httpResponse.responseBody, 'SIL_INVOLVEDPARTYorganization', 'json');
                  IF($response.silBusinessOrganization != null, $silIds.silBusinessOrganization.addElement($response.silBusinessOrganization.id));
                  //ElseIf $response.hasFailed() Then
                  //   Return $response;
               End
               /***************** GET AFFILIATE ***************************************************/
               $httpResponse := INT_SILHttpClient.getInvolvedPartyIdAffiliate($partyId);
               $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
               If $response.getLastCallHasSucceededAndHasContent() Then
                  $response.silAffiliate := TranslateFrom($httpResponse.responseBody, 'SIL_INVOLVEDPARTYorganization:indexed', 'json');
                  ForEach SIL_INVOLVEDPARTYorganization $silAffiliate In $response.silAffiliate Do
                     IF($silAffiliate != null, $silIds.silAffiliate.addElement($silAffiliate.id));
                  End
                  //ElseIf $response.hasFailed() Then
                  //   Return $response;
               End
            Else
               /***************** NATURE OF BUSINESS **************************************/
            End
         End
      End
      If NOTEQUAL(CMN_KYCLevel.hide(), $kycLevel) Then
         /***************** GET TAX-INFORMATION **************************************/
         $httpResponse := INT_SILHttpClient.getInvolvedPartyIdTaxInformation($partyId);
         $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
         If $response.getLastCallHasSucceededAndHasContent() Then
            $response.silTaxInformation := TranslateFrom($httpResponse.responseBody, 'SIL_INVOLVEDPARTYtaxInformation', 'json');
         End
         If NOTEQUAL(CMN_KYCLevel.none(), $kycLevel) Then
            /***************** GET SCC **************************************/
            $httpResponse := INT_SILHttpClient.getInvolvedPartyIdScc($partyId);
            $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
            If $response.getLastCallHasSucceededAndHasContent() Then
               $response.silSCC := TranslateFrom($httpResponse.responseBody, 'SIL_CUSTOMERCLASSIFICATIONspecialCategoryCustomer:indexed', 'json');
            End
            /***************** GET SCREENING  **************************************/
            $httpResponse := INT_SILHttpClient.getInvolvedPartyIdScreening($partyId);
            $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
            If $response.getLastCallHasSucceededAndHasContent() Then
               $response.silScreening := TranslateFrom($httpResponse.responseBody, 'SIL_INVOLVEDPARTYSCREENINGscreening:indexed', 'json');
               ForEach SIL_INVOLVEDPARTYSCREENINGscreening $silScreening In $response.silScreening Do
                  IF($silScreening != null, $silIds.silScreening.addElement($silScreening.id));
               End
            End
         End
         If EQUAL(CMN_KYCLevel.full(), $kycLevel) Then
            /***************** FINANCIAL INFORMATION && OverallNetWorth **************************************/
            $httpResponse := INT_SILHttpClient.getInvolvedPartyIdEstimatedNetWorth($partyId);
            $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
            If $response.getLastCallHasSucceededAndHasContent() Then
               $response.silEstimatedNetWorth := TranslateFrom($httpResponse.responseBody, 'SIL_KYCestimatedNetWorth:indexed', 'json');
               //TO REVIEW
               IF(LIB_Validator.validateNotEmptyIndexed($response.silEstimatedNetWorth), $silIds.silEstimatedNetWorth.addElement($response.silEstimatedNetWorth[1].id));
               //ElseIf $response.hasFailed() Then
               //   Return $response;
            End
            /***************** GET ACCOUNT SUMMARY LIST *****************************************/
            $httpResponse := INT_SILHttpClient.getInvolvedPartyIdAccountSummaryList($partyId);
            $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
            If $response.getLastCallHasSucceededAndHasContent() Then
               $response.silAccountSummaryList := TranslateFrom($httpResponse.responseBody, 'SIL_ACCOUNTaccount:indexed', 'json');
               // ElseIf $response.hasFailed() Then
               //    Return $response;
            End
            /***************** GET CONNECTEDPARTIES *********************************************/
            If $response.silParties == null Then
               $response.silParties := NewIndexed(Record);
               $httpResponse := INT_SILHttpClient.getInvolvedPartyIdConnectedParty($partyId);
               $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
               If $response.getLastCallHasSucceededAndHasContent() Then
                  $response.silConnectedParty := TranslateFrom($httpResponse.responseBody, 'SIL_INVOLVEDPARTYinvolvedPartyRelationship:indexed', 'json');
                  SIL_INVOLVEDPARTYinvolvedPartyRelationship $relationShip;
                  ForEach $relationShip In $response.silConnectedParty Do
                     Record $newSilParty := NewObject(Record);
                     SIL_INVOLVEDPARTYinvolvedParty $connectedParty := ERROR($relationShip.party, null);
                     If $connectedParty != null and NotEmpty($connectedParty.id) Then
                        $newSilParty.setValue('id', $connectedParty.id);
                        $newSilParty.setValue('relationshipType', $relationShip.relationshipType);
                        $newSilParty.setValue('relationshipRole', $relationShip.relationshipRole);
                        $newSilParty.setValue('relationshipSubType', $relationShip.relationshipSubType);
                        $newSilParty.setValue('silParty', $relationShip.party);
                        $response.silParties.addElement($newSilParty);
                     End
                  End
               End
            End
            /***************** GET ACCOUNT ACTIVITY ********************************************/
            /* If LIB_Validator.validateNotEmptyIndexed($response.silAccountSummaryList) Then
         ForEach SIL_ACCOUNTaccount $accountSummary In $response.silAccountSummaryList Do
            SIL_ACCOUNTaccount $silAccount := null;
            If not $silReadAccounts.containsKey($accountSummary.accountId) Then
               $httpResponse := INT_SILHttpClient.getAccountIdDetails($accountSummary.accountId);
               $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
               If $response.getLastCallHasSucceededAndHasContent() Then
                  $silAccount := TranslateFrom($httpResponse.responseBody, 'SIL_ACCOUNTaccount', 'json');
                  $silReadAccounts[$accountSummary.accountId] := $silAccount;*/
            /*If not $readAccountActivityResponse.containsKey($accountSummary.accountId) Then
               INT_SILReadAccountActivityResponse $activityResponse := INT_SILProxyClient.readAccountActivity($accountSummary.accountId);
               $response.addServiceStatusFromServiceResponses($activityResponse);
               If $activityResponse.getLastCallHasSucceededAndHasContent() Then
                  $response.cmnAccountActivities[$accountSummary.accountId] := $activityResponse.kytExpectedAccountActivity;
                  $readAccountActivityResponse[$accountSummary.accountId] := $activityResponse;
                  //$silReadAccounts[$accountSummary.accountId] := $activityResponse.kytExpectedAccountActivity;
               Else
                  $readAccountActivityResponse[$accountSummary.accountId] := null;
               End
            End*/
            /*Else
                  $silReadAccounts[$accountSummary.accountId] := null;
               End
            Else
               $silAccount := $silReadAccounts[$accountSummary.accountId];
            End
            If $silAccount != null Then
               $response.silAccountDetailsList.addElement($silAccount);
            End
         End
      End*/
         End
      End
      INT_SILMappingSILtoCMN.convertSILInvolvedPartyToCMNParty($response);
   Catch
      $response.addServiceStatusFromException($exception);
      LIB_Logger.error('No party was created correctly: ' & $exception.getMessage());
   End
   Long $end := TIMESTAMP();
   Return $response;
End]]></definition>
        </method>
        <method name="readPersonIDV" userId="pezzi" uuid="ffffffff-c113-7230-0000-000000000000">
            <definition><![CDATA[/**
  Description of function 'readPersonIDV'.
  @param $value Description of parameter '$value'.
  @return       Description of return value.
*/
StaticFunction readPersonIDV(String $partyId, CMN_Party $cmnParty := null) : INT_SILReadPartyResponse Begin
   Long $before := TIMESTAMP();
   INT_SILReadPartyResponse $response := NewObject(INT_SILReadPartyResponse);
   INT_HttpResponseWrapper $httpResponse;
   Try
      /***************** GET DETAILS AND DIRECTLY DESERIALISE (ROOT) ******************/
      $httpResponse := INT_SILHttpClient.getInvolvedPartyIdDetails($partyId);
      $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
      If $response.getLastCallHasSucceededAndHasContent() Then
         $response.silInvolvedPartyDetails := TranslateFrom($httpResponse.responseBody, 'SIL_INVOLVEDPARTYinvolvedParty', 'json');
      Else
         // Break chain as root object was not found
         Long $end := TIMESTAMP();
         LIB_Logger.error('No party was found: ' & $partyId);
         LIB_Logger.warn('Time taken to complete readPerson with root object was not found request in milliseconds: ' & TOSTRING(($end - $before)));
         Return $response;
      End
      $response.party := $cmnParty;
      $response.odbParty := CLONE($cmnParty);
      INT_SILMappingSILtoCMN.convertSILInvolvedPartyIDVToCMNParty($response);
   Catch
      $response.addServiceStatusFromException($exception);
      LIB_Logger.error('No party was created correctly: ' & $exception.getMessage());
   End
   Long $end := TIMESTAMP();
   Return $response;
End]]></definition>
        </method>
        <method name="readPersonToPersonRelations" userId="pezzi" uuid="00000000-774c-6cdc-0000-000000000000">
            <definition><![CDATA[StaticFunction readPersonToPersonRelations(CMN_Party $cmnParty, CMN_Root $root := null, CMN_BusinessRelationship $br, Indexed Record $connectedParties, Named INT_SILReadPartyResponse $readPartyResponses) : INT_SILReadPartyResponse Begin
   /***************** GET CONNECTEDPARTIES *********************************************/
   INT_SILReadPartyResponse $response := $readPartyResponses[$cmnParty.getSILID()];
   If $response == null Then
      $response := NEW(INT_SILReadPartyResponse);
      $readPartyResponses[$cmnParty.getSILID()] := $response;
   End
   $response.silParties := $connectedParties;
   If $response.silParties == null Then
      INT_HttpResponseWrapper $httpResponse := INT_SILHttpClient.getInvolvedPartyIdConnectedParty($cmnParty.getSILID());
      $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
      If $response.getLastCallHasSucceededAndHasContent() Then
         Indexed SIL_INVOLVEDPARTYinvolvedPartyRelationship $silConnectedParty := TranslateFrom($httpResponse.responseBody, 'SIL_INVOLVEDPARTYinvolvedPartyRelationship:indexed', 'json');
         $response.silParties := NewIndexed(Record);
         SIL_INVOLVEDPARTYinvolvedPartyRelationship $relationShip;
         ForEach $relationShip In $silConnectedParty Do
            Record $newSilParty := NewObject(Record);
            SIL_INVOLVEDPARTYinvolvedParty $connectedParty := ERROR($relationShip.party, null);
            If $connectedParty != null and NotEmpty($connectedParty.id) Then
               $newSilParty.setValue('id', $connectedParty.id);
               $newSilParty.setValue('SILID', $relationShip.id);
               $newSilParty.setValue('relationshipType', $relationShip.relationshipType);
               If EMPTY($connectedParty.id) or EMPTY($relationShip.relationshipType) Then
                  Continue;
               End
               String $cmnRole := LIST($relationShip.relationshipType, 'CMN_PartyRoles', 'id', 'sil_Id');
               //Linked Account Connected party Role
               If EMPTY($cmnRole) Then
                  $newSilParty.setValue('relationshipRole', $relationShip.relationshipRole);
                  $newSilParty.setValue('relationshipSubType', $relationShip.relationshipSubType);
                  $newSilParty.setValue('silParty', $relationShip.party);
                  $response.silParties.addElement($newSilParty);
               Else
                  //Account Party to Party Role
                  If $readPartyResponses[$connectedParty.id] != null Then
                     //$connectedPartyRecord.setValue('CMN_Party', $readPartyResponses[$partyId]);
                  ElseIf $readPartyResponses.containsKey($connectedParty.id) Then
                     //was previously read but not successfull
                     Continue;
                  Else
                     CMN_Party $otherParty;
                     INT_SILReadPartyResponse $readPartyResponse := INT_SILIntegrationProcess.readPartyBasicInformation($connectedParty.id, $root);
                     $response.addServiceStatusFromServiceResponses($readPartyResponse);
                     If $readPartyResponse.exists() and $readPartyResponse.party != null Then
                        $readPartyResponses[$connectedParty.id] := $readPartyResponse;
                     Else
                        $readPartyResponses[$connectedParty.id] := null;
                        Continue;
                     End
                  End
                  CMN_Party $cmnConnectedParty := $readPartyResponses[$connectedParty.id].party;
                  If $cmnConnectedParty != null and $cmnConnectedParty.type != null Then
                     CMN_PartyRoleManager $cmnPartyRoleManager := $cmnParty.getRoleManager($br);
                     CMN_ApplicablePartyRole $role := INT_SILMappingSILtoCMN._createCMNPartyRole($cmnPartyRoleManager, $cmnRole);
                     If $role != null Then
                        $cmnPartyRoleManager.addRoleForParty($cmnConnectedParty, $role);
                     End
                  End
               End
            End
         End
      End
   End
   Return $response;
End]]></definition>
        </method>
        <method name="readRelation" userId="pezzi" uuid="ffffffff-88b3-c1b2-0000-000000000000">
            <definition><![CDATA[/**
  Description of function 'readRelation'.
  @param $value Description of parameter '$value'.
  @return       Description of return value.
*/
StaticFunction readRelation(String $accountId, String $useCase := '', CMN_ClientLifecycleData $clientLifeCycleData := null, CMN_BusinessRelationship $cmnBusinessRelationship := null, Named SIL_ACCOUNTaccount $silReadAccounts) : INT_SILReadAccountResponse Begin
   Long $before := TIMESTAMP();
   /***************************************** GENERAL FIELDS **************************************/
   INT_SILReadAccountResponse $response := NewObject(INT_SILReadAccountResponse);
   SIL_ACCOUNTaccount $silAccount;
   INT_HttpResponseWrapper $httpResponse;
   String $responseBody;
   CMN_SilIds $silIds;
   Try
      If $silReadAccounts == null Then
         $silReadAccounts := NewNamed(SIL_ACCOUNTaccount);
      End
      If $clientLifeCycleData == null Then
         $silIds := NEW(CMN_SilIds);
      Else
         $silIds := $clientLifeCycleData.getSILIDs($accountId);
         If $silIds == null Then
            $silIds := NEW(CMN_SilIds);
         End
      End
      /***************************************** READ ACCOUNT DETAILS ********************************/
      If $silReadAccounts[$accountId] != null Then
         $response.silAccountDetails := $silReadAccounts[$accountId];
      Else
         $httpResponse := INT_SILHttpClient.getAccountIdDetails($accountId);
         $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
         If $response.getLastCallHasSucceededAndHasContent() Then
            $response.silAccountDetails := TranslateFrom($httpResponse.responseBody, 'SIL_ACCOUNTaccount', 'json');
            If EQUAL($response.silAccountDetails.clientNature, 'Joint Entity Client') Then
               $useCase := 'joint';
            End
