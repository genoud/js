   End
   Long $end := TIMESTAMP();
   LIB_Logger.warn('Time taken to complete checkPerson request in milliseconds: ' & TOSTRING(($end - $before)));
   Return $silResponse;
End]]></definition>
        </method>
        <method name="checkRelation" userId="pezzi" uuid="00000000-6ee0-bb24-0000-000000000000">
            <definition><![CDATA[/**
  Check relation if it exists and if it is in prebooked state.
  @param $accountId is the id of the account to be checked.
  @return Response instance containing two flags: exists and isPrebooked
*/
StaticFunction checkRelation(String $accountId) : INT_SILCheckAccountResponse Begin
   INT_SILCheckAccountResponse $silResponse := NewObject(INT_SILCheckAccountResponse);
   INT_HttpResponseWrapper $httpResponse;
   /*********************** REQUEST FOR SUMMARY *************************************/
   $httpResponse := INT_SILHttpClient.getAccountIdSummary($accountId);
   $silResponse.addServiceStatusFromHttpResponseWrapper($httpResponse);
   /***********************DESERIALISE REQUEST **************************************/
   If $silResponse.hasSucceded() Then
      Try
         If $silResponse.getLastCallHasContent() Then
            $silResponse.accountSummary := TranslateFrom($httpResponse.responseBody, 'SIL_ACCOUNTaccount', 'json');
         End
      Catch
         $silResponse.addServiceStatusFromException($exception);
      End
   End
   Return $silResponse;
End]]></definition>
        </method>
        <method name="commitOrder" userId="pezzi" uuid="00000000-593b-9857-0000-000000000000">
            <definition><![CDATA[/**
  Description of function 'createOrder'.
  @param $value Description of parameter '$value'.
  @return       Description of return value.
*/
StaticFunction commitOrder(String $id, String $type, INT_SILTransaction $transaction) : INT_SILTransaction Begin
   Long $before := TIMESTAMP();
   String $requestBody;
   INT_HttpResponseWrapper $httpResponse;
   If $transaction != null && NotEmpty($transaction.transactionId) Then
      /********* BP ORDER ***********/
      $transaction.addCommitTransactionHeader();
      If $type == 'account' Then
         /********* BP ORDER ***********/
         $requestBody := TranslateTo(CAST(SIL_ACCOUNTaccount, CreateSkeletonDataClass(SIL_ACCOUNTaccount)), 'json');
         $httpResponse := INT_SILHttpClient.putAcccount($id, $requestBody, $transaction.getHeaders());
      Else
         If $type == 'physical' Then
            $requestBody := TranslateTo(CAST(SIL_INVOLVEDPARTYindividual, CreateSkeletonDataClass(SIL_INVOLVEDPARTYindividual)), 'json');
         ElseIf $type == 'trust' Then
            $requestBody := TranslateTo(CAST(SIL_INVOLVEDPARTYtrust, CreateSkeletonDataClass(SIL_INVOLVEDPARTYtrust)), 'json');
         Else
            $requestBody := TranslateTo(CAST(SIL_INVOLVEDPARTYorganization, CreateSkeletonDataClass(SIL_INVOLVEDPARTYorganization)), 'json');
         End
         $httpResponse := INT_SILHttpClient.putInvolvedParty($id, $requestBody, $transaction.getHeaders());
      End
         $transaction.serviceResponse.addServiceStatusFromHttpResponseWrapper($httpResponse);
    
   End
   Return $transaction;
End]]></definition>
        </method>
        <method name="createOrder" userId="pezzi" uuid="ffffffff-e1b2-33b2-0000-000000000000">
            <definition><![CDATA[/**
  Description of function 'createOrder'.
  @param $value Description of parameter '$value'.
  @return       Description of return value.
*/
StaticFunction createOrder(String $id, String $type) : INT_SILTransaction Begin
   Long $before := TIMESTAMP();
   INT_ServiceResponse $response;
   String $requestBody;
   INT_HttpResponseWrapper $httpResponse;
   INT_SILTransaction $transaction;
   If $type == 'account' Then
      /********* BP ORDER ***********/
      $response := NewObject(INT_SILStoreRelationResponse);
      $transaction := NewObject(INT_SILTransaction, $response);
      $requestBody := TranslateTo(CAST(SIL_ACCOUNTaccount, CreateSkeletonDataClass(SIL_ACCOUNTaccount)), 'json');
      $httpResponse := INT_SILHttpClient.putAcccount($id, $requestBody, $transaction.getHeaders());
      $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
   Else
      /********* PARtY ORDER ***********/
      $response := NewObject(INT_SILStorePartyResponse);
      $transaction := NewObject(INT_SILTransaction, $response);
      If $type == 'physical' Then
         $requestBody := TranslateTo(CAST(SIL_INVOLVEDPARTYindividual, CreateSkeletonDataClass(SIL_INVOLVEDPARTYindividual)), 'json');
      ElseIf $type == 'trust' Then
         /********* PARtY ORDER ***********/
         $requestBody := TranslateTo(CAST(SIL_INVOLVEDPARTYtrust, CreateSkeletonDataClass(SIL_INVOLVEDPARTYtrust)), 'json');
      Else
         /********* PARtY ORDER ***********/
         $requestBody := TranslateTo(CAST(SIL_INVOLVEDPARTYorganization, CreateSkeletonDataClass(SIL_INVOLVEDPARTYorganization)), 'json');
      End
      $httpResponse := INT_SILHttpClient.putInvolvedParty($id, $requestBody, $transaction.getHeaders());
      $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
   End
   If $response != null and $response.getLastCallHasSucceededAndHasContent() Then
      $transaction.addTransactionIdHeader();
   End
   Return $transaction;
End]]></definition>
        </method>
        <method name="createPerson" userId="pezzi" uuid="00000000-5598-7b71-0000-000000000000">
            <definition><![CDATA[/*
	Store a given party (Customer in SIL terms) to the SIL Backend. 
    @params $party is a pointer to the party to be stored
    @params $cmnBusinessRelationship is a pointer to the businessrelationship assigned to the given party
    @return INT_SILStorePartyResponse contains the stored party and the message corresponding to the success type (failed / success)
*/
StaticFunction createPerson(CMN_Party $cmnParty, String $kycLevel, CMN_BusinessRelationship $cmnBusinessRelationship, CMN_ClientLifecycleData $cld := null, INT_SILTransaction $transaction := null, String $partyId := '') : INT_SILStorePartyResponse Begin
   Long $before := TIMESTAMP();
   /************************************ VARIABLES ********************************************/
   INT_SILStorePartyResponse $response := NewObject(INT_SILStorePartyResponse);
   IF(NotEmpty($cmnParty.getSILID()), $partyId := $cmnParty.getSILID());
   If EMPTY($partyId) Then
      Return $response;
   End
   INT_HttpResponseWrapper $httpResponse;
   SIL_INVOLVEDPARTYInvolvedParty $silInvolvedParty;
   CMN_SilIds $silIds;
   If $cld == null Then
      $silIds := NEW(CMN_SilIds);
   Else
      $silIds := CLONE($cld.getSILIDs($partyId));
      If $silIds == null Then
         $silIds := CLONE($cld.setSILDs($partyId));
      End
   End
   String $requestBody;
   Boolean $rollBack := false;
   /************************************ CONVERT CMN PARTY TO SIL PARTY ***********************/
   Try
      If $cmnParty == null || $cmnBusinessRelationship == null Then
         Return $response;
      Else
         $response.silInvolvedParty := INT_SILMappingCMNtoSIL.convertCMNPartyToSILInvolvedParty($cmnParty);
         If not $cmnParty.isTakenFromSIL Then
            $response.silInvolvedParty := INT_SILMappingCMNtoSIL.createSILInvolvedPartyIDV($cmnParty, $cmnBusinessRelationship, $response.silInvolvedParty);
         End
         $response.silInvolvedParty := INT_SILMappingCMNtoSIL.createSILCustomerRole($cmnParty, $cmnBusinessRelationship, $response.silInvolvedParty);
         //$response.silInvolvedParty.screenings := INT_SILMappingCMNtoSIL.createSILScreeningScreening($cmnParty);
      End
      /************************************ STORE DETAILS (ROOT) ****************************/
      //If not $cmnParty.isTakenFromSIL or NOT(EMPTY($cmnParty.getManualFCCRRCode())) Then
      $httpResponse := INT_SILHttpClient.putInvolvedParty($partyId, TranslateTo($response.silInvolvedParty, 'json'), $transaction.getHeaders());
      $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
      If $response.getLastCallHasSucceededAndHasContent() Then
         $response.silInvolvedParty := TranslateFrom($httpResponse.responseBody, 'SIL_INVOLVEDPARTYinvolvedParty', 'json');
      Else
         //FAIL('Error in Involved Party '&$response.silInvolvedParty.id);
         Return $response;
      End
      //End
      If $cmnParty.type.isLegalEntity() Then
         If EQUAL($kycLevel, CMN_KYCLevel.full()) Then
            /********************************** STORE NATURE OF BUSINESS & SOURCE OF WEALTH ***********************/
            If $cmnParty.type.isCommercialOperatingBusiness() Then
               SIL_INVOLVEDPARTYorganization $silOrganization := INT_SILMappingCMNtoSIL.createSILBusinessOrganization($cmnParty);
               If $silOrganization != null Then
                  $requestBody := TranslateTo($silOrganization, 'json');
                  If NotEmpty($silOrganization.id) Then
                     $httpResponse := INT_SILHttpClient.putInvolvedPartyIdBusinessOrganization($silOrganization.id, $requestBody, $transaction.getHeaders());
                  Else
                     $httpResponse := INT_SILHttpClient.postInvolvedPartyIdBusinessOrganization($partyId, $requestBody, $transaction.getHeaders());
                  End
                  $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
                  If $response.getLastCallHasSucceededAndHasContent() Then
                     SIL_INVOLVEDPARTYorganization $receivedSilOrganization := TranslateFrom($httpResponse.responseBody, 'SIL_INVOLVEDPARTYorganization', 'json');
                     $silOrganization.id := $receivedSilOrganization.id;
                     $response.silBusinessOrganization := $silOrganization;
                  ElseIf $response.hasFailed() Then
                     Return $response;
                  End
               End
            End
            /************************************ STORE AFFILIATES*****************************/
            // Serialise affiliates to generate list
            Indexed SIL_INVOLVEDPARTYorganization $silAffiliates := INT_SILMappingCMNtoSIL.createSILAffiliate($cmnParty);
            For Integer $i := 1 Condition $i <= TOINTEGER(MIN($silAffiliates.size(), LIB_ConfigHelper.getMaximumNumberOfAffiliate())) Step $i := $i + 1 Do
               SIL_INVOLVEDPARTYorganization $silAffiliate := $silAffiliates[$i];
               $requestBody := TranslateTo($silAffiliate, 'json');
               If NotEmpty($silAffiliate.id) Then
                  $httpResponse := INT_SILHttpClient.putInvolvedPartyAffiliateId($silAffiliate.id, $requestBody, $transaction.getHeaders());
                  $silIds.silAffiliate.removeElement($silAffiliate.id);
               Else
                  $httpResponse := INT_SILHttpClient.postInvolvedPartyIdAffiliate($partyId, $requestBody, $transaction.getHeaders());
               End
               $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
               If $response.getLastCallHasSucceededAndHasContent() Then
                  SIL_INVOLVEDPARTYorganization $receivedsilAffiliate := TranslateFrom($httpResponse.responseBody, 'SIL_INVOLVEDPARTYorganization', 'json');
                  $silAffiliate.id := $receivedsilAffiliate.id;
                  $response.silAffiliate.addElement($silAffiliate);
               End
               If $response.hasFailed() Then
                  Return $response;
               End
            End
            ForEach String $silAffiliateToDelete In $silIds.silAffiliate Do
               If NotEmpty($silAffiliateToDelete) Then
                  $httpResponse := INT_SILHttpClient.deleteInvolvedPartyAffiliateId($silAffiliateToDelete, null, $transaction.getHeaders());
                  $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
                  If $response.getLastCallHasSucceeded() Then
                     $response.silAffiliatesDeleted.addElement($silAffiliateToDelete);
                  End
               End
            End
         End
      ElseIf $cmnParty.type.isPhysicalPerson() Then
         If EQUAL($kycLevel, CMN_KYCLevel.basicPlus()) or EQUAL($kycLevel, CMN_KYCLevel.full()) Then
            /************************************ STORE SOW *************/
            Indexed SIL_SOWindividualSourceOfWealth $silSOWs := INT_SILMappingCMNtoSIL.createSILSOW($cmnParty);
            ForEach SIL_SOWindividualSourceOfWealth $silSOW In $silSOWs Do
               $requestBody := TranslateTo($silSOW, 'json');
               If NotEmpty($silSOW.id) Then
                  $silIds.silSOW.removeElement($silSOW.id);
                  $httpResponse := INT_SILHttpClient.putInvolvedPartySOWId($silSOW.id, $requestBody, $transaction.getHeaders());
               Else
                  $httpResponse := INT_SILHttpClient.postInvolvedPartyIdSOW($partyId, $requestBody, $transaction.getHeaders());
               End
               $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
               If $response.getLastCallHasSucceededAndHasContent() Then
                  SIL_SOWindividualSourceOfWealth $receivedSilSOW := TranslateFrom($httpResponse.responseBody, 'SIL_SOWindividualSourceOfWealth', 'json');
                  $silSOW.id := $receivedSilSOW.id;
                  $response.silIndividualSourceOfWealth.addElement($silSOW);
                  //CAST(CMN_PhysicalPerson, $cmnParty).getKYCProfile().sourceOfWealth.setSILID($silSOW.id);
               ElseIf $response.hasFailed() Then
                  Return $response;
               End
            End
            ForEach String $silSOWToDelete In $silIds.silSOW Do
               If NotEmpty($silSOWToDelete) Then
                  $httpResponse := INT_SILHttpClient.deleteInvolvedPartySOWId($silSOWToDelete, null, $transaction.getHeaders());
                  $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
                  If $response.getLastCallHasSucceeded() Then
                     $response.silSOWDeleted.addElement($silSOWToDelete);
                  End
               End
            End
         End
      End
      If NOTEQUAL($kycLevel, CMN_KYCLevel.hide()) Then
         /********************************** STORE TAX-INFORMATION ************************************************/
         If not $cmnParty.isTakenFromSIL Then
            SIL_INVOLVEDPARTYtaxInformation $silTaxInformation := INT_SILMappingCMNtoSIL.createSILTaxInformation($cmnParty);
            If $silTaxInformation != null Then
               $requestBody := TranslateTo($silTaxInformation, 'json');
               If EMPTY($silTaxInformation.id) Then
                  $httpResponse := INT_SILHttpClient.postInvolvedPartyIdTaxInformation($partyId, $requestBody, $transaction.getHeaders());
                  $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
                  If $response.getLastCallHasSucceededAndHasContent() Then
                     SIL_INVOLVEDPARTYtaxInformation $receivedSilTaxInformation := TranslateFrom($httpResponse.responseBody, 'SIL_INVOLVEDPARTYtaxInformation', 'json');
                     $silTaxInformation.id := $receivedSilTaxInformation.id;
                     $response.silTaxInfomation := $silTaxInformation;
                  ElseIf $response.hasFailed() Then
                     Return $response;
                  End
               End
            End
         End
         If NOTEQUAL($kycLevel, CMN_KYCLevel.none()) Then
            /********************************** STORE SCC **************************************/
            SIL_CUSTOMERCLASSIFICATIONspecialCategoryCustomer $silSpecialCategoryCustomer := INT_SILMappingCMNtoSIL.createSILSpecialCategoryCustomer($cmnParty);
            //ForEach SIL_CUSTOMERCLASSIFICATIONspecialCategoryCustomer $silSpecialCategoryCustomer In $customerClassifications Do
            If $silSpecialCategoryCustomer != null Then
               $requestBody := TranslateTo($silSpecialCategoryCustomer, 'json');
               If NotEmpty($silSpecialCategoryCustomer.id) Then
                  //$silIds.silSCC.removeElement($silSpecialCategoryCustomer.id);
                  $httpResponse := INT_SILHttpClient.putInvolvedPartySccId($silSpecialCategoryCustomer.id, $requestBody, $transaction.getHeaders());
               Else
                  $httpResponse := INT_SILHttpClient.postInvolvedPartyIdScc($partyId, $requestBody, $transaction.getHeaders());
               End
               $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
               If $response.getLastCallHasSucceededAndHasContent() Then
                  SIL_CUSTOMERCLASSIFICATIONspecialCategoryCustomer $receivedSilSpecialCategoryCustomer := TranslateFrom($httpResponse.responseBody, 'SIL_CUSTOMERCLASSIFICATIONspecialCategoryCustomer', 'json');
                  $silSpecialCategoryCustomer.id := $receivedSilSpecialCategoryCustomer.id;
                  $response.silSpecialCategoryCustomer := $receivedSilSpecialCategoryCustomer;
               ElseIf $response.hasFailed() Then
                  Return $response;
               End
            End
            // End
            /*ForEach String $silSCCToDelete In $silIds.silSCC Do
               If NotEmpty($silSCCToDelete) Then
                  $httpResponse := INT_SILHttpClient.deleteInvolvedPartySccId($silSCCToDelete, null, $transaction.getHeaders());
               End
            End*/
            /********************************** STORE SCREENING **************************************/
            Indexed SIL_INVOLVEDPARTYSCREENINGscreening $silScreenings := INT_SILMappingCMNtoSIL.createSILScreeningScreening($cmnParty);
            SIL_INVOLVEDPARTYSCREENINGscreening $silScreening;
            ForEach $silScreening In $silScreenings Do
               $requestBody := TranslateTo($silScreening, 'json');
               $httpResponse := INT_SILHttpClient.postInvolvedPartyIdScreening($partyId, $requestBody, $transaction.getHeaders());
               //End
               $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
               If $response.getLastCallHasSucceededAndHasContent() Then
                  SIL_INVOLVEDPARTYSCREENINGscreening $receivedSilScreening := TranslateFrom($httpResponse.responseBody, 'SIL_INVOLVEDPARTYSCREENINGscreening', 'json');
                  $silScreening.id := $receivedSilScreening.id;
                  $response.silScreening.addElement($silScreening);
                  //ERROR($negativeNews[$i].setSILID($silScreening.id), null);
               ElseIf $response.hasFailed() Then
                  Return $response;
               End
            End
            /*ForEach String $silScreeningToDelete In $silIds.silScreening Do
               If NotEmpty($silScreeningToDelete) Then
                  $httpResponse := INT_SILHttpClient.deleteInvolvedPartyScreeningId($silScreeningToDelete, null, $transaction.getHeaders());
               End
            End*/
            /********************************** STORE SCREENING Findings**************************************/
            Indexed SIL_INVOLVEDPARTYSCREENINGfinding $silFindings := INT_SILMappingCMNtoSIL.createSILFindings($cmnParty);
            SIL_INVOLVEDPARTYSCREENINGfinding $silFinding;
            ForEach $silFinding In $silFindings Do
               $requestBody := TranslateTo($silFinding, 'json');
               If NotEmpty($silFinding.id) Then
                  $silIds.silFindings.removeElement($silFinding.id);
                  $httpResponse := INT_SILHttpClient.putInvolvedPartyFindingId($silFinding.id, $requestBody, $transaction.getHeaders());
               Else
                  $httpResponse := INT_SILHttpClient.postInvolvedPartyIdFinding($partyId, $requestBody, $transaction.getHeaders());
               End
               //End
               $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
               If $response.getLastCallHasSucceededAndHasContent() Then
                  SIL_INVOLVEDPARTYSCREENINGfinding $receivedSilFinding := TranslateFrom($httpResponse.responseBody, 'SIL_INVOLVEDPARTYSCREENINGfinding', 'json');
                  $silFinding.id := $receivedSilFinding.id;
                  $response.silFindings.addElement($silFinding);
                  //ERROR($negativeNews[$i].setSILID($silScreening.id), null);
               ElseIf $response.hasFailed() Then
                  Return $response;
               End
            End
            ForEach String $silFindingToDelete In $silIds.silFindings Do
               If NotEmpty($silFindingToDelete) Then
                  $httpResponse := INT_SILHttpClient.deleteInvolvedPartyFindingId($silFindingToDelete, null, $transaction.getHeaders());
                  $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
                  If $response.getLastCallHasSucceeded() Then
                     $response.silFindingsDeleted.addElement($silFindingToDelete);
                  End
               End
            End
            If EQUAL($kycLevel, CMN_KYCLevel.full()) Then
               /********************************** STORE FINANCIAL INFORMATION OR OVERALL NET WORTH **************************************/
               SIL_KYCestimatedNetWorth $silKYCestimatedNetWorth;
               If $cmnParty.type.isLegalEntity() Then
                  $silKYCestimatedNetWorth := INT_SILMappingCMNtoSIL.createSILEstimatedNetWorth($cmnParty);
               Else
                  $silKYCestimatedNetWorth := INT_SILMappingCMNtoSIL.createSILOverallNetWorth($cmnParty);
               End
               If $silKYCestimatedNetWorth != null Then
                  $requestBody := TranslateTo($silKYCestimatedNetWorth, 'json');
                  If NotEmpty($silKYCestimatedNetWorth.id) Then
                     $httpResponse := INT_SILHttpClient.putInvolvedPartyEstimatedNetWorthId($silKYCestimatedNetWorth.id, $requestBody, $transaction.getHeaders());
                  Else
                     $httpResponse := INT_SILHttpClient.postInvolvedPartyIdEstimatedNetWorth($partyId, $requestBody, $transaction.getHeaders());
                  End
                  $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
                  If $response.getLastCallHasSucceededAndHasContent() Then
                     SIL_KYCestimatedNetWorth $receivedSilKYCestimatedNetWorth := TranslateFrom($httpResponse.responseBody, 'SIL_KYCestimatedNetWorth', 'json');
                     $silKYCestimatedNetWorth.id := $receivedSilKYCestimatedNetWorth.id;
                     $response.silEstimatedNetWorth := $receivedSilKYCestimatedNetWorth;
                     //CAST(CMN_LegalEntity, $cmnParty).getKYCProfile().financialInformation.setSILID($silKYCestimatedNetWorth.id);
                  ElseIf $response.hasFailed() Then
                     Return $response;
                  End
               End
            End
         End
      End
      /************************************ STORE CONNECTED PARTIES ************************/
      // Serialise Connected Parties to get the number of connected parties, create for each one an entry
      Indexed SIL_INVOLVEDPARTYinvolvedPartyRelationship $partyStructure;
      If EQUAL($kycLevel, CMN_KYCLevel.full()) Then
         $partyStructure := INT_SILMappingCMNtoSIL.createSILRelationships($cmnParty);
      End
      If not $cmnParty.isTakenFromSIL Then
         $partyStructure := INT_SILMappingCMNtoSIL.createSILConnectedParties($cmnParty, $cmnBusinessRelationship, $partyStructure);
      End
      SIL_INVOLVEDPARTYinvolvedPartyRelationship $partyRelationship;
      ForEach $partyRelationship In $partyStructure Do
         If $partyRelationship != null Then
            $requestBody := TranslateTo($partyRelationship, 'json');
            If NotEmpty($partyRelationship.id) Then
               $httpResponse := INT_SILHttpClient.putInvolvedPartyConnectedPartyId($partyRelationship.id, $requestBody, $transaction.getHeaders());
            Else
               $httpResponse := INT_SILHttpClient.postInvolvedPartyIdConnectedParty($partyId, $requestBody, $transaction.getHeaders());
            End
            $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
            If $response.getLastCallHasSucceededAndHasContent() Then
               SIL_INVOLVEDPARTYinvolvedPartyRelationship $receivedInvolvedPartyRelationship := TranslateFrom($httpResponse.responseBody, 'SIL_INVOLVEDPARTYinvolvedPartyRelationship', 'json');
               $partyRelationship.id := $receivedInvolvedPartyRelationship.id;
               $response.silConnectedParty.addElement($partyRelationship);
            ElseIf $response.hasFailed() Then
               Return $response;
            End
         End
      End
   Catch
      $response.addServiceStatusFromException($exception);
      $rollBack := true;
      LIB_Logger.integrationError('RollBack :' & $exception.getMessage());
   Finally
      //$response.hasFailed() or 
      If $rollBack and NotEmpty($transaction.transactionId) Then
         Try
            $transaction.addRollbackTransactionHeader();
            $httpResponse := INT_SILHttpClient.putInvolvedParty($partyId, TranslateTo(CAST(SIL_INVOLVEDPARTYindividual, CreateSkeletonDataClass(SIL_INVOLVEDPARTYindividual)), 'json'), $transaction.getHeaders());
            $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
         Catch
            LIB_Logger.integrationError('Error while doing rollback :' & $exception.getMessage());
            $response.addServiceStatusFromException($exception);
         End
      End
      Long $end := TIMESTAMP();
      LIB_Logger.warn('Time taken to complete createPerson request when error occurs in milliseconds: ' & TOSTRING(($end - $before)));
      Return $response;
   End
   /************************************ PERSISTED IF ALL CALLS TECHNICALLY SUCCESSFUL ********/
   Long $end := TIMESTAMP();
   LIB_Logger.warn('Time taken to complete createPerson request in milliseconds: ' & TOSTRING(($end - $before)));
   Return $response;
End]]></definition>
        </method>
        <method name="createPersonBestEffort" userId="bellanca" uuid="00000000-6a62-6641-0000-000000000000">
            <definition><![CDATA[/*
	Store a given party (Customer in SIL terms) to the SIL Backend. 
    @params $party is a pointer to the party to be stored
    @params $cmnBusinessRelationship is a pointer to the businessrelationship assigned to the given party
    @return INT_SILStorePartyResponse contains the stored party and the message corresponding to the success type (failed / success)
*/
StaticFunction createPersonBestEffort(CMN_Party $cmnParty, String $kycLevel, CMN_BusinessRelationship $cmnBusinessRelationship, CMN_ClientLifecycleData $cld := null, INT_SILTransaction $transaction := null, String $partyId := '') : INT_SILStorePartyResponse Begin
   Long $before := TIMESTAMP();
   /************************************ VARIABLES ********************************************/
   INT_SILStorePartyResponse $response := NewObject(INT_SILStorePartyResponse);
   IF(NotEmpty($cmnParty.getSILID()), $partyId := $cmnParty.getSILID());
   If EMPTY($partyId) Then
      Return $response;
   End
   INT_HttpResponseWrapper $httpResponse;
   SIL_INVOLVEDPARTYInvolvedParty $silInvolvedParty;
   CMN_SilIds $silIds;
   If $cld == null Then
      $silIds := NEW(CMN_SilIds);
   Else
      $silIds := CLONE($cld.getSILIDs($partyId));
      If $silIds == null Then
         $silIds := CLONE($cld.setSILDs($partyId));
      End
   End
   String $requestBody;
   Boolean $rollBack := false;
   Boolean $transactionStarted := false;
   // Order Management
   If $transaction == null or EMPTY($transaction.transactionId) Then
      //$transactionStarted := true;
      $transaction := NewObject(INT_SILTransaction, $response);
   End
   /************************************ CONVERT CMN PARTY TO SIL PARTY ***********************/
   Try
      If $cmnParty == null || $cmnBusinessRelationship == null Then
         Return $response;
      Else
         $response.silInvolvedParty := INT_SILMappingCMNtoSIL.convertCMNPartyToSILInvolvedParty($cmnParty);
         If not $cmnParty.isTakenFromSIL Then
            $response.silInvolvedParty := INT_SILMappingCMNtoSIL.createSILInvolvedPartyIDV($cmnParty, $cmnBusinessRelationship, $response.silInvolvedParty);
            $response.silInvolvedParty := INT_SILMappingCMNtoSIL.createSILAccountHolderRole($cmnParty, $cmnBusinessRelationship, $response.silInvolvedParty);
         End
         $response.silInvolvedParty := INT_SILMappingCMNtoSIL.createSILCustomerRole($cmnParty, $cmnBusinessRelationship, $response.silInvolvedParty);
      End
      /************************************ STORE DETAILS (ROOT) ****************************/
      //If not $cmnParty.isTakenFromSIL or NOT(EMPTY($cmnParty.getManualFCCRRCode())) Then
      $httpResponse := INT_SILHttpClient.putInvolvedParty($partyId, TranslateTo($response.silInvolvedParty, 'json'), $transaction.getHeaders());
      $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
      If $response.getLastCallHasSucceededAndHasContent() Then
         IF($transactionStarted, $transaction.addTransactionIdHeader());
         $response.silInvolvedParty := TranslateFrom($httpResponse.responseBody, 'SIL_INVOLVEDPARTYinvolvedParty', 'json');
      Else
         //FAIL('Error in Involved Party '&$response.silInvolvedParty.id);
         //Return $response;
      End
      //End
      If $cmnParty.type.isLegalEntity() Then
         If EQUAL($kycLevel, CMN_KYCLevel.full()) Then
            /********************************** STORE BUSINESS ORGANIZATION ***********************/
            SIL_INVOLVEDPARTYorganization $silOrganization := INT_SILMappingCMNtoSIL.createSILBusinessOrganization($cmnParty);
            If $silOrganization != null Then
               $requestBody := TranslateTo($silOrganization, 'json');
               If $cmnParty.isTakenFromSIL Then
                  $httpResponse := INT_SILHttpClient.putInvolvedPartyIdBusinessOrganization($partyId, $requestBody, $transaction.getHeaders());
               Else
                  $httpResponse := INT_SILHttpClient.postInvolvedPartyIdBusinessOrganization($partyId, $requestBody, $transaction.getHeaders());
               End
               $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
               If $response.getLastCallHasSucceededAndHasContent() Then
                  SIL_INVOLVEDPARTYorganization $receivedSilOrganization := TranslateFrom($httpResponse.responseBody, 'SIL_INVOLVEDPARTYorganization', 'json');
                  $silOrganization.id := $receivedSilOrganization.id;
                  $response.silBusinessOrganization := $silOrganization;
               ElseIf $response.hasFailed() Then
                  //  Return $response;
               End
            End
            /************************************ STORE AFFILIATES*****************************/
            // Serialise affiliates to generate list
            Indexed SIL_INVOLVEDPARTYorganization $silAffiliates := INT_SILMappingCMNtoSIL.createSILAffiliate($cmnParty);
            For Integer $i := 1 Condition $i <= TOINTEGER(MIN($silAffiliates.size(), LIB_ConfigHelper.getMaximumNumberOfAffiliate())) Step $i := $i + 1 Do
               SIL_INVOLVEDPARTYorganization $silAffiliate := $silAffiliates[$i];
               $requestBody := TranslateTo($silAffiliate, 'json');
               If NotEmpty($silAffiliate.id) Then
                  $httpResponse := INT_SILHttpClient.putInvolvedPartyAffiliateId($silAffiliate.id, $requestBody, $transaction.getHeaders());
                  $silIds.silAffiliate.removeElement($silAffiliate.id);
               Else
                  $httpResponse := INT_SILHttpClient.postInvolvedPartyIdAffiliate($partyId, $requestBody, $transaction.getHeaders());
               End
               $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
               If $response.getLastCallHasSucceededAndHasContent() Then
                  SIL_INVOLVEDPARTYorganization $receivedsilAffiliate := TranslateFrom($httpResponse.responseBody, 'SIL_INVOLVEDPARTYorganization', 'json');
                  $silAffiliate.id := $receivedsilAffiliate.id;
                  $response.silAffiliate.addElement($silAffiliate);
               End
               If $response.hasFailed() Then
                  //  Return $response;
               End
            End
            ForEach String $silAffiliateToDelete In $silIds.silAffiliate Do
               If NotEmpty($silAffiliateToDelete) Then
                  $httpResponse := INT_SILHttpClient.deleteInvolvedPartyAffiliateId($silAffiliateToDelete, null, $transaction.getHeaders());
                  $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
                  If $response.getLastCallHasSucceeded() Then
                     $response.silAffiliatesDeleted.addElement($silAffiliateToDelete);
                  End
               End
            End
         End
      ElseIf $cmnParty.type.isPhysicalPerson() Then
         If EQUAL($kycLevel, CMN_KYCLevel.basicPlus()) or EQUAL($kycLevel, CMN_KYCLevel.full()) Then
            /************************************ STORE SOW *************/
            Indexed SIL_SOWindividualSourceOfWealth $silSOWs := INT_SILMappingCMNtoSIL.createSILSOW($cmnParty);
            ForEach SIL_SOWindividualSourceOfWealth $silSOW In $silSOWs Do
               $requestBody := TranslateTo($silSOW, 'json');
               If NotEmpty($silSOW.id) Then
                  $silIds.silSOW.removeElement($silSOW.id);
                  $httpResponse := INT_SILHttpClient.putInvolvedPartySOWId($silSOW.id, $requestBody, $transaction.getHeaders());
               Else
                  $httpResponse := INT_SILHttpClient.postInvolvedPartyIdSOW($partyId, $requestBody, $transaction.getHeaders());
               End
               $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
               If $response.getLastCallHasSucceededAndHasContent() Then
                  SIL_SOWindividualSourceOfWealth $receivedSilSOW := TranslateFrom($httpResponse.responseBody, 'SIL_SOWindividualSourceOfWealth', 'json');
                  $silSOW.id := $receivedSilSOW.id;
                  $response.silIndividualSourceOfWealth.addElement($silSOW);
                  //CAST(CMN_PhysicalPerson, $cmnParty).getKYCProfile().sourceOfWealth.setSILID($silSOW.id);
               ElseIf $response.hasFailed() Then
                  //  Return $response;
               End
            End
            ForEach String $silSOWToDelete In $silIds.silSOW Do
               If NotEmpty($silSOWToDelete) Then
                  $httpResponse := INT_SILHttpClient.deleteInvolvedPartySOWId($silSOWToDelete, null, $transaction.getHeaders());
                  $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
                  If $response.getLastCallHasSucceeded() Then
                     $response.silSOWDeleted.addElement($silSOWToDelete);
                  End
               End
            End
         End
      End
      If NOTEQUAL($kycLevel, CMN_KYCLevel.hide()) Then
         /********************************** STORE TAX-INFORMATION ************************************************/
         If not $cmnParty.isTakenFromSIL Then
            SIL_INVOLVEDPARTYtaxInformation $silTaxInformation := INT_SILMappingCMNtoSIL.createSILTaxInformation($cmnParty);
            If $silTaxInformation != null Then
               $requestBody := TranslateTo($silTaxInformation, 'json');
               If EMPTY($silTaxInformation.id) Then
                  $httpResponse := INT_SILHttpClient.postInvolvedPartyIdTaxInformation($partyId, $requestBody, $transaction.getHeaders());
                  $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
                  If $response.getLastCallHasSucceededAndHasContent() Then
                     SIL_INVOLVEDPARTYtaxInformation $receivedSilTaxInformation := TranslateFrom($httpResponse.responseBody, 'SIL_INVOLVEDPARTYtaxInformation', 'json');
                     $silTaxInformation.id := $receivedSilTaxInformation.id;
                     $response.silTaxInfomation := $silTaxInformation;
                  ElseIf $response.hasFailed() Then
                     //  Return $response;
                  End
               End
            End
         End
         If NOTEQUAL($kycLevel, CMN_KYCLevel.none()) Then
            /********************************** STORE SCC **************************************/
            SIL_CUSTOMERCLASSIFICATIONspecialCategoryCustomer $silSpecialCategoryCustomer := INT_SILMappingCMNtoSIL.createSILSpecialCategoryCustomer($cmnParty);
            //ForEach SIL_CUSTOMERCLASSIFICATIONspecialCategoryCustomer $silSpecialCategoryCustomer In $customerClassifications Do
            If $silSpecialCategoryCustomer != null Then
               $requestBody := TranslateTo($silSpecialCategoryCustomer, 'json');
               If NotEmpty($silSpecialCategoryCustomer.id) Then
                  //$silIds.silSCC.removeElement($silSpecialCategoryCustomer.id);
                  $httpResponse := INT_SILHttpClient.putInvolvedPartySccId($silSpecialCategoryCustomer.id, $requestBody, $transaction.getHeaders());
               Else
                  $httpResponse := INT_SILHttpClient.postInvolvedPartyIdScc($partyId, $requestBody, $transaction.getHeaders());
               End
               $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
               If $response.getLastCallHasSucceededAndHasContent() Then
                  SIL_CUSTOMERCLASSIFICATIONspecialCategoryCustomer $receivedSilSpecialCategoryCustomer := TranslateFrom($httpResponse.responseBody, 'SIL_CUSTOMERCLASSIFICATIONspecialCategoryCustomer', 'json');
                  $silSpecialCategoryCustomer.id := $receivedSilSpecialCategoryCustomer.id;
                  $response.silSpecialCategoryCustomer := $receivedSilSpecialCategoryCustomer;
               ElseIf $response.hasFailed() Then
                  //  Return $response;
               End
            End
            // End
            /*ForEach String $silSCCToDelete In $silIds.silSCC Do
               If NotEmpty($silSCCToDelete) Then
                  $httpResponse := INT_SILHttpClient.deleteInvolvedPartySccId($silSCCToDelete, null, $transaction.getHeaders());
               End
            End*/
            /********************************** STORE SCREENING **************************************/
            Indexed SIL_INVOLVEDPARTYSCREENINGscreening $silScreenings := INT_SILMappingCMNtoSIL.createSILScreeningScreening($cmnParty);
            SIL_INVOLVEDPARTYSCREENINGscreening $silScreening;
            ForEach $silScreening In $silScreenings Do
               $requestBody := TranslateTo($silScreening, 'json');
               $httpResponse := INT_SILHttpClient.postInvolvedPartyIdScreening($partyId, $requestBody, $transaction.getHeaders());
               //End
               $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
               If $response.getLastCallHasSucceededAndHasContent() Then
                  SIL_INVOLVEDPARTYSCREENINGscreening $receivedSilScreening := TranslateFrom($httpResponse.responseBody, 'SIL_INVOLVEDPARTYSCREENINGscreening', 'json');
                  $silScreening.id := $receivedSilScreening.id;
                  $response.silScreening.addElement($silScreening);
                  //ERROR($negativeNews[$i].setSILID($silScreening.id), null);
               ElseIf $response.hasFailed() Then
                  //  Return $response;
               End
            End
            /*ForEach String $silScreeningToDelete In $silIds.silScreening Do
               If NotEmpty($silScreeningToDelete) Then
                  $httpResponse := INT_SILHttpClient.deleteInvolvedPartyScreeningId($silScreeningToDelete, null, $transaction.getHeaders());
               End
            End*/
            /********************************** STORE SCREENING Findings**************************************/
            Indexed SIL_INVOLVEDPARTYSCREENINGfinding $silFindings := INT_SILMappingCMNtoSIL.createSILFindings($cmnParty);
            SIL_INVOLVEDPARTYSCREENINGfinding $silFinding;
            ForEach $silFinding In $silFindings Do
               $requestBody := TranslateTo($silFinding, 'json');
               If NotEmpty($silFinding.id) Then
                  $silIds.silFindings.removeElement($silFinding.id);
                  $httpResponse := INT_SILHttpClient.putInvolvedPartyFindingId($silFinding.id, $requestBody, $transaction.getHeaders());
               Else
                  $httpResponse := INT_SILHttpClient.postInvolvedPartyIdFinding($partyId, $requestBody, $transaction.getHeaders());
               End
               //End
               $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
               If $response.getLastCallHasSucceededAndHasContent() Then
                  SIL_INVOLVEDPARTYSCREENINGfinding $receivedSilFinding := TranslateFrom($httpResponse.responseBody, 'SIL_INVOLVEDPARTYSCREENINGfinding', 'json');
                  $silFinding.id := $receivedSilFinding.id;
                  $response.silFindings.addElement($silFinding);
                  //ERROR($negativeNews[$i].setSILID($silScreening.id), null);
               ElseIf $response.hasFailed() Then
                  //  Return $response;
               End
            End
            ForEach String $silFindingToDelete In $silIds.silFindings Do
               If NotEmpty($silFindingToDelete) Then
                  $httpResponse := INT_SILHttpClient.deleteInvolvedPartyFindingId($silFindingToDelete, null, $transaction.getHeaders());
                  $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
                  If $response.getLastCallHasSucceeded() Then
                     $response.silFindingsDeleted.addElement($silFindingToDelete);
                  End
               End
            End
            If EQUAL($kycLevel, CMN_KYCLevel.full()) Then
               /********************************** STORE FINANCIAL INFORMATION OR OVERALL NET WORTH **************************************/
               SIL_KYCestimatedNetWorth $silKYCestimatedNetWorth;
               If $cmnParty.type.isLegalEntity() Then
                  $silKYCestimatedNetWorth := INT_SILMappingCMNtoSIL.createSILEstimatedNetWorth($cmnParty);
               Else
                  $silKYCestimatedNetWorth := INT_SILMappingCMNtoSIL.createSILOverallNetWorth($cmnParty);
               End
               If $silKYCestimatedNetWorth != null Then
                  $requestBody := TranslateTo($silKYCestimatedNetWorth, 'json');
                  If NotEmpty($silKYCestimatedNetWorth.id) Then
                     $httpResponse := INT_SILHttpClient.putInvolvedPartyEstimatedNetWorthId($silKYCestimatedNetWorth.id, $requestBody, $transaction.getHeaders());
                  Else
                     $httpResponse := INT_SILHttpClient.postInvolvedPartyIdEstimatedNetWorth($partyId, $requestBody, $transaction.getHeaders());
                  End
                  $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
                  If $response.getLastCallHasSucceededAndHasContent() Then
                     SIL_KYCestimatedNetWorth $receivedSilKYCestimatedNetWorth := TranslateFrom($httpResponse.responseBody, 'SIL_KYCestimatedNetWorth', 'json');
                     $silKYCestimatedNetWorth.id := $receivedSilKYCestimatedNetWorth.id;
                     $response.silEstimatedNetWorth := $receivedSilKYCestimatedNetWorth;
                     //CAST(CMN_LegalEntity, $cmnParty).getKYCProfile().financialInformation.setSILID($silKYCestimatedNetWorth.id);
                  ElseIf $response.hasFailed() Then
                     //  Return $response;
                  End
               End
            End
         End
      End
      /********************************** ACCOUNT HOLDER SERVICE **************************************/
      If $cmnBusinessRelationship.partyIsCustomer($cmnParty) Then
         Indexed SIL_RELATIONservice $ahServices := INT_SILMappingCMNtoSIL.createSILAccountServices($cmnBusinessRelationship);
         If LIB_Validator.validateNotEmptyIndexed($ahServices) Then
            ForEach SIL_RELATIONservice $silAHService In $ahServices Do
               If $silAHService != null and not $cmnBusinessRelationship.isTakenFromSIL Then
                  $requestBody := TranslateTo($silAHService, 'json');
                  $httpResponse := INT_SILHttpClient.postInvolvedPartyPartyIdAccountIdService($partyId, $cmnBusinessRelationship.getSILID(), $requestBody, $transaction.getHeaders());
                  //End
                  $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
                  If $response.getLastCallHasSucceededAndHasContent() Then
                  ElseIf $response.hasFailed() Then
                     //  Return $response;
                  End
               End
            End
         End
      End
      /********************************** CLIENT SERVICE **************************************/
      Indexed SIL_RELATIONservice $clientServices := INT_SILMappingCMNtoSIL.createSILClientServices($cmnBusinessRelationship, $cmnParty);
      If LIB_Validator.validateNotEmptyIndexed($clientServices) Then
         ForEach SIL_RELATIONservice $silClientService In $clientServices Do
            If $silClientService != null and not $cmnBusinessRelationship.isTakenFromSIL Then
               $requestBody := TranslateTo($silClientService, 'json');
               $httpResponse := INT_SILHttpClient.postInvolvedPartyPartyIdAccountIdService($partyId, $cmnBusinessRelationship.getSILID(), $requestBody, $transaction.getHeaders());
               //End
               $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
               If $response.getLastCallHasSucceededAndHasContent() Then
               ElseIf $response.hasFailed() Then
                  //  Return $response;
               End
            End
         End
      End
      /************************************ STORE CONNECTED PARTIES ************************/
      // Serialise Connected Parties to get the number of connected parties, create for each one an entry
      Indexed SIL_INVOLVEDPARTYinvolvedPartyRelationship $partyStructure;
      If EQUAL($kycLevel, CMN_KYCLevel.full()) Then
         $partyStructure := INT_SILMappingCMNtoSIL.createSILRelationships($cmnParty);
      End
      If not $cmnParty.isTakenFromSIL Then
         $partyStructure := INT_SILMappingCMNtoSIL.createSILConnectedParties($cmnParty, $cmnBusinessRelationship, $partyStructure);
      End
      SIL_INVOLVEDPARTYinvolvedPartyRelationship $partyRelationship;
      ForEach $partyRelationship In $partyStructure Do
         If $partyRelationship != null Then
            $requestBody := TranslateTo($partyRelationship, 'json');
            If NotEmpty($partyRelationship.id) Then
               $httpResponse := INT_SILHttpClient.putInvolvedPartyConnectedPartyId($partyRelationship.id, $requestBody, $transaction.getHeaders());
            Else
               $httpResponse := INT_SILHttpClient.postInvolvedPartyIdConnectedParty($partyId, $requestBody, $transaction.getHeaders());
            End
            $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
            If $response.getLastCallHasSucceededAndHasContent() Then
               SIL_INVOLVEDPARTYinvolvedPartyRelationship $receivedInvolvedPartyRelationship := TranslateFrom($httpResponse.responseBody, 'SIL_INVOLVEDPARTYinvolvedPartyRelationship', 'json');
               $partyRelationship.id := $receivedInvolvedPartyRelationship.id;
               $response.silConnectedParty.addElement($partyRelationship);
            ElseIf $response.hasFailed() Then
               //  Return $response;
            End
         End
      End
      /************************************ FINALISE STORE BY UPDATING INVOLVEDPARTY *************/
      If $transactionStarted && NotEmpty($transaction.transactionId) Then
         $transaction.addCommitTransactionHeader();
         $httpResponse := INT_SILHttpClient.putInvolvedParty($partyId, TranslateTo(CAST(SIL_INVOLVEDPARTYindividual, CreateSkeletonDataClass(SIL_INVOLVEDPARTYindividual)), 'json'), $transaction.getHeaders());
         $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
      End
   Catch
      $response.addServiceStatusFromException($exception);
      $rollBack := true;
      LIB_Logger.integrationError('RollBack :' & $exception.getMessage());
   Finally
      //$response.hasFailed() or 
      If $transactionStarted && ($rollBack) and NotEmpty($transaction.transactionId) Then
         Try
            $transaction.addRollbackTransactionHeader();
            $httpResponse := INT_SILHttpClient.putInvolvedParty($partyId, TranslateTo(CAST(SIL_INVOLVEDPARTYindividual, CreateSkeletonDataClass(SIL_INVOLVEDPARTYindividual)), 'json'), $transaction.getHeaders());
            $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
         Catch
            LIB_Logger.integrationError('Error while doing rollback :' & $exception.getMessage());
            $response.addServiceStatusFromException($exception);
         End
      End
      Long $end := TIMESTAMP();
      LIB_Logger.warn('Time taken to complete createPerson request when error occurs in milliseconds: ' & TOSTRING(($end - $before)));
      Return $response;
   End
   /************************************ PERSISTED IF ALL CALLS TECHNICALLY SUCCESSFUL ********/
   Long $end := TIMESTAMP();
   LIB_Logger.warn('Time taken to complete createPerson request in milliseconds: ' & TOSTRING(($end - $before)));
   Return $response;
End]]></definition>
        </method>
        <method name="createRelation" userId="pezzi" uuid="00000000-15f1-4318-0000-000000000000">
            <definition><![CDATA[/**
  Store Relation / Business relationship
  @param $businessRelationship to be added
  @return       Response object
*/
StaticFunction createRelation(CMN_BusinessRelationship $cmnBusinessRelationship, INT_SILTransaction $transaction := null, String $accountId := '') : INT_SILStoreRelationResponse Begin
   Long $before := TIMESTAMP();
   /************************************ VARIABLES ********************************************/
   INT_SILStoreRelationResponse $response := NewObject(INT_SILStoreRelationResponse);
   IF(NotEmpty($cmnBusinessRelationship.getSILID()), $accountId := $cmnBusinessRelationship.getSILID());
   If EMPTY($accountId) Then
      Return $response;
   End
   INT_HttpResponseWrapper $httpResponse;
   String $requestBody;
   Indexed CMN_Party $customers;
   Boolean $rollBack := false;
   If $cmnBusinessRelationship != null Then
      $customers := $cmnBusinessRelationship.getCustomers();
   End
   Try
      /************************************ SERIALISE AND SEND Account Details ********************************************/
      If not $cmnBusinessRelationship.isTakenFromSIL Then
         $response.silAccount := INT_SILMappingCMNtoSIL.createSILAccount($cmnBusinessRelationship, $response.silAccount);
      End
      If not $cmnBusinessRelationship.kytProfile.visitation.isExistingVisitedCustomer Then
         $response.silAccount := INT_SILMappingCMNtoSIL.createSILAccountProfile($cmnBusinessRelationship, $response.silAccount);
      End
      $response.silAccount := INT_SILMappingCMNtoSIL.createSILAccountPurpose($cmnBusinessRelationship, $response.silAccount);
      $requestBody := TranslateTo($response.silAccount, 'json');
      $httpResponse := INT_SILHttpClient.putAcccount($accountId, $requestBody, $transaction.getHeaders());
      $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
      If $response.getLastCallHasSucceededAndHasContent() Then
         SIL_ACCOUNTaccount $receivedSilAccount := TranslateFrom($httpResponse.responseBody, 'SIL_ACCOUNTaccount', 'json');
         $response.silAccount := $receivedSilAccount;
      ElseIf $response.getLastCallHasFailed() Then
         Return $response;
      End
      /********************************** STORE TAX-INFORMATION ************************************************/
      SIL_INVOLVEDPARTYtaxInformation $silTaxInformation := INT_SILMappingCMNtoSIL.createSILAccountTaxInformation($cmnBusinessRelationship);
      If $silTaxInformation != null Then
         $requestBody := TranslateTo($silTaxInformation, 'json');
         If NotEmpty($silTaxInformation.id) Then
            $httpResponse := INT_SILHttpClient.putAccountTaxInformationId($silTaxInformation.id, $requestBody, $transaction.getHeaders());
         Else
            $httpResponse := INT_SILHttpClient.postAccountIdTaxInformation($accountId, $requestBody, $transaction.getHeaders());
         End
         $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
         If $response.getLastCallHasSucceededAndHasContent() Then
            IF(EMPTY($transaction.transactionId), $transaction.addTransactionIdHeader());
            $silTaxInformation := TranslateFrom($httpResponse.responseBody, 'SIL_INVOLVEDPARTYtaxInformation', 'json');
            $cmnBusinessRelationship.regulatoryProfile.setSILID($silTaxInformation.id);
         ElseIf $response.hasFailed() Then
            Return $response;
         End
      End
      /************************************ ACCOUNT FUNDING **********************************************/
      $response.silAccountFunding := INT_SILMappingCMNtoSIL.createSILAccountFunding($cmnBusinessRelationship);
      $requestBody := TranslateTo($response.silAccountFunding, 'json');
      If NotEmpty($response.silAccountFunding.id) Then
         $httpResponse := INT_SILHttpClient.putAccountFundingId($accountId, $requestBody, $transaction.getHeaders());
      Else
         $httpResponse := INT_SILHttpClient.postAccountIdFunding($accountId, $requestBody, $transaction.getHeaders());
      End
      $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
      If $response.getLastCallHasSucceededAndHasContent() Then
         SIL_ACCOUNTaccountFunding $receivedSilAccountFunding := TranslateFrom($httpResponse.responseBody, 'SIL_ACCOUNTaccountFunding', 'json');
         $response.silAccountFunding.id := $receivedSilAccountFunding.id;
         //$cmnBusinessRelationship.kytProfile.initialFunding.setSILID($response.silAccountFunding.id);
      ElseIf $response.getLastCallHasFailed() Then
         $response.silAccountFunding := null;
         Return $response;
      End
      /************************************ ACCOUNT Transfer Activity ***************************************/
      Indexed SIL_KYCaccountTransferActivity $silKYCaccountTransferActivities := INT_SILMappingCMNtoSIL.createSILAccountTransferActivity($cmnBusinessRelationship);
      For Integer $i := 1 Condition $i <= $silKYCaccountTransferActivities.size() Step $i := $i + 1 Do
         SIL_KYCaccountTransferActivity $silKYCaccountTransferActivity := $silKYCaccountTransferActivities[$i];
         $requestBody := TranslateTo($silKYCaccountTransferActivity, 'json');
         If NotEmpty($silKYCaccountTransferActivity.id) Then
            $httpResponse := INT_SILHttpClient.putAccountKycTransferActivityId($silKYCaccountTransferActivity.id, $requestBody, $transaction.getHeaders());
         Else
            $httpResponse := INT_SILHttpClient.postAccountIdKycTransferActivity($accountId, $requestBody, $transaction.getHeaders());
         End
         $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
         If $response.getLastCallHasSucceededAndHasContent() Then
            SIL_KYCaccountTransferActivity $receivedSilKYCaccountTransferActivity := TranslateFrom($httpResponse.responseBody, 'SIL_KYCaccountTransferActivity', 'json');
            $silKYCaccountTransferActivity.id := $receivedSilKYCaccountTransferActivity.id;
            $response.silKycAccountTransferActivity.addElement($silKYCaccountTransferActivity);
            /*If $silKYCaccountTransferActivity != null and NotEmpty($silKYCaccountTransferActivity.transferType) Then
               If ISA($silKYCaccountTransferActivity, SIL_KYCaccountTransferInActivity) Then
                  $cmnBusinessRelationship.kytProfile.expectedAccountActivity.transferIn.silIds[$silKYCaccountTransferActivity.transferType] := $silKYCaccountTransferActivity.id;
               ElseIf ISA($silKYCaccountTransferActivity, SIL_KYCaccountTransferOutActivity) Then
                  $cmnBusinessRelationship.kytProfile.expectedAccountActivity.transferOut.silIds[$silKYCaccountTransferActivity.transferType] := $silKYCaccountTransferActivity.id;
               End
            End*/
         ElseIf $response.getLastCallHasFailed() Then
            $silKYCaccountTransferActivity := null;
            Return $response;
         End
      End
      /************************************ ONLY COB ***************************************/
      If not $cmnBusinessRelationship.isTakenFromSIL Then
         /************************************ ACCOUNT FUNDING TRANSFER **********************************************/
         Indexed SIL_ORDERtransfer $silOrderTransfer := INT_SILMappingCMNtoSIL.createSILAccountFundingTransfer($cmnBusinessRelationship);
         /*Integer $chequeIndex := 1;
         Integer $wireIndex := 1;
         Integer $securitiesIndex := 1;*/
         ForEach SIL_ORDERtransfer $accountFundingTransfer In $silOrderTransfer Do
            //If EMPTY($accountFundingTransfer.id) Then
            $requestBody := TranslateTo($accountFundingTransfer, 'json');
            $httpResponse := INT_SILHttpClient.postAccountIdFundingTransfer($accountId, $requestBody, $transaction.getHeaders());
            $response.addServiceStatusFromHttpResponseWrapper($httpResponse);
            If $response.getLastCallHasSucceededAndHasContent() Then
               SIL_ORDERtransfer $receivedAccountFundingTransfer := TranslateFrom($httpResponse.responseBody, 'SIL_ORDERtransfer', 'json');
               $accountFundingTransfer.id := $receivedAccountFundingTransfer.id;
               $response.silOrderTrasnfer.addElement($accountFundingTransfer);
               /*If $accountFundingTransfer.type == '5' or $accountFundingTransfer.type == '3' or $accountFundingTransfer.type == '4' Then
                     If $accountFundingTransfer.type == '5' Then
                        $cmnBusinessRelationship.kytProfile.initialFunding.cheque[$chequeIndex].setSILID($receivedAccountFundingTransfer.id);
                        $chequeIndex := $chequeIndex + 1;
                     ElseIf $accountFundingTransfer.type == '3' Then
                        $cmnBusinessRelationship.kytProfile.initialFunding.wireTransfers[$wireIndex].setSILID($receivedAccountFundingTransfer.id);
                        $wireIndex := $wireIndex + 1;
                     ElseIf $accountFundingTransfer.type == '4' Then
                        $cmnBusinessRelationship.kytProfile.initialFunding.securities[$securitiesIndex].setSILID($receivedAccountFundingTransfer.id);
                        $securitiesIndex := $securitiesIndex + 1;
                     End
                  Else
                     If $accountFundingTransfer.type == '6' Then
                        $cmnBusinessRelationship.kytProfile.initialFunding.phyAssetsCashAmountId := $receivedAccountFundingTransfer.id;
                     ElseIf $accountFundingTransfer.type == '8' Then
                        $cmnBusinessRelationship.kytProfile.initialFunding.phyAssetsPreciousMetalAmountId := $receivedAccountFundingTransfer.id;
                     ElseIf $accountFundingTransfer.type == '9' Then
                        $cmnBusinessRelationship.kytProfile.initialFunding.phyAssetsSecuritiesAmountId := $receivedAccountFundingTransfer.id;
                     End
                  End*/
