<?xml version="1.0" encoding="UTF-8"?>
<scriptfunction xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="scriptfunction.xsd">
    <definition><![CDATA[Function CMN_PrefillOptionalService__defv(CMN_BusinessRelationship $br) : Nothing Begin
   // CMN_BusinessRelationship $br := $root.getBusinessRelationship();
   $br.optionalServices.refreshAvailableOptions($br);
   If ! $br.optionalServices.isOptionSelected('purposeOfAccount') Then
      $br.optionalServices.toggleOptionSelection('purposeOfAccount');
   End
   If LIB_Validator.validateNotEmptyIndexed($br.getRoleManager().getPartiesInRole('creditCardHolder')) and not $br.optionalServices.hasUserChangedOption('creditCard') and not $br.optionalServices.isOptionSelected('creditCard') Then
      $br.optionalServices.toggleOptionSelection('creditCard');
   End
   If LIB_Validator.validateNotEmptyIndexed($br.getRoleManager().getPartiesInRole('safeHolder')) and not $br.optionalServices.hasUserChangedOption('safeDepositBox') and not $br.optionalServices.isOptionSelected('safeHolder') Then
      $br.optionalServices.toggleOptionSelection('safeDepositBox');
   End
   If LIB_Validator.validateNotEmptyIndexed($br.getRoleManager().getPartiesInRole('internetBanking')) and not $br.optionalServices.hasUserChangedOption('internetAccess') and not $br.optionalServices.isOptionSelected('internetBanking') Then
      $br.optionalServices.toggleOptionSelection('internetAccess');
      $br.optionalServices.toggleOptionSelection('internetBankingServices');
   End
   If LIB_Validator.validateNotEmptyIndexed($br.getRoleManager().getPartiesInRole('internetBankingOperating')) and not $br.optionalServices.hasUserChangedOption('internetBankingOperatingService') and not $br.optionalServices.isOptionSelected('internetBankingOperatingService') Then
      $br.optionalServices.toggleOptionSelection('internetBankingOperatingService');
      If not $br.optionalServices.hasUserChangedOption('internetBankingServices') and not $br.optionalServices.isOptionSelected('internetBankingServices') Then
         $br.optionalServices.toggleOptionSelection('internetBankingServices');
      End
   End
   If LIB_Validator.validateNotEmptyIndexed($br.getRoleManager().getPartiesInRole('internetBankingViewOnly')) and not $br.optionalServices.hasUserChangedOption('internetBankingViewOnlyService') and not $br.optionalServices.isOptionSelected('internetBankingViewOnlyService') Then
      $br.optionalServices.toggleOptionSelection('internetBankingViewOnlyService');
      If not $br.optionalServices.hasUserChangedOption('internetBankingServices') and not $br.optionalServices.isOptionSelected('internetBankingServices') Then
         $br.optionalServices.toggleOptionSelection('internetBankingServices');
      End
   End
End]]></definition>
</scriptfunction>

